module simple_counter(
    CLOCK,
    counter_out,
);
input CLOCK;
output [3:0] counter_out;   //This is the 4-bit counter
reg [3:0] counter_out;

always @(posedge CLOCK) begin   // on positive clock edge
    counter_out <= counter_out + 1;     //increment counter
    
end
endmodule // simple_counter